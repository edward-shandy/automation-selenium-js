{
  "name": "chromedriver-manager",
  "version": "0.2.0",
  "description": "A manager for the chromedriver binary and related miscellany",
  "main": " ",
  "scripts": {
    "build": "coffee --bare --compile --output . src/ && coffee build/scripts.coffee",
    "prepublish": "npm run-script build"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/goodeggs/chromedriver-manager.git"
  },
